                                               - <a href="{{path('sortie_annuler',{'id':result.sortie.id})}}">annuler</a>
                                        {%endif%}
                                {%endif%}
                        {% else %}
                                {% if result.sortie.etat.libelle == 'En création' %}

                                {% elseif result.sortie.etat.libelle == 'Ouvert' %}<a href="{{path('sortie_affiche',{'id':result.sortie.id})}}">Afficher</a>helo3

                                        -  {% if result.inscrit %}<a href="{{path('sortie_desister',{'id':result.sortie.id})}}">se désister</a>{% else %}<a href="{{path('sortie_inscrire',{'id':result.sortie.id})}}">s'inscrire</a>{% endif %}

                                {% elseif result.sortie.etat.libelle == 'Fermé' %}<a href="{{path('sortie_affiche',{'id':result.sortie.id})}}">Afficher</a>helo2 {{result.inscrit}}

                                        -  {% if result.inscrit %}<a href="{{path('sortie_desister',{'id':result.sortie.id})}}">se désister</a>{% endif %}

                                {% else %}<a href="{{path('sortie_affiche',{'id':result.sortie.id})}}">Afficher</a>helo1

                                {% endif %}
                                {% if app.user.roles[0]  == "ROLE_ADMIN" and result.sortie.etat.libelle != "Annulé" and result.sortie.etat.libelle !='En cours' and result.sortie.etat.libelle !='Terminé' %}
                                        <a href="{{path('sortie_annuler',{'id':result.sortie.id})}}">annuler</a>
                                {%endif%}
                        {% endif %}
                </td>
        </tr>
