                                    <a href="{{path('sortie_affiche',{'id':result.sortie.id})}}">Afficher</a>
                                {% elseif result.sortie.etat.libelle == 'Fermé' %}
                                    <a href="{{path('sortie_affiche',{'id':result.sortie.id})}}">Afficher</a>
                                {% else %}<a href="{{path('sortie_affiche',{'id':result.sortie.id})}}">Afficher</a>
                                {% endif %}
                                {% if app.user.roles[0]  == "ROLE_ADMIN" or  app.user.id == result.sortie.organisateur.id %}
